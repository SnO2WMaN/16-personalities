{
  "private": true,
  "scripts": {
    "dev": "run-p -sln dev:*",
    "dev:next": "next dev",
    "dev:i18ngen": "typesafe-i18n",
    "build": "run-s -cln i18ngen build:next",
    "build:next": "next build",
    "i18ngen": "typesafe-i18n --no-watch",
    "typecheck": "tsc --noEmit",
    "lint": "run-s -cln lint:*",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:prettier": "prettier --check **/*.{css,json}",
    "fmt": "run-s -cln fmt:*",
    "fmt:eslint": "yarn lint:eslint --fix",
    "fmt:prettier": "yarn lint:prettier --write"
  },
  "engines": {
    "node": "18.19.1"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.2.4",
    "styled-components": "^5.3.1",
    "typesafe-i18n": "^2.37.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@shopify/eslint-plugin": "^40.4.0",
    "@shopify/prettier-config": "^1.1.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.13",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-unused-imports": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "react-is": "^17.0.2",
    "tailwindcss": "^2.2.9",
    "typescript": "^4.3.5"
  }
}
